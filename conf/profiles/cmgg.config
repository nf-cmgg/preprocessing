docker {
    enabled         = true
    userEmulation   = true
    runOptions      = "--platform linux/x86_64"
}

params {
    config_profile_description  = 'CMGG Slurm config'
    config_profile_contact      = '@matthdsm'
    max_memory                  = 630.GB
    max_cpus                    = 75
    max_time                    = 72.h
}
process {
    executor = 'slurm'
    queue = 'debug'
    maxRetries = 2
}

report {
    overwrite = true
}
