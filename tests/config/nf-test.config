params {
    // Limit resources so that this can run on GitHub Actions
    max_cpus   = 2
    max_memory = '6.GB'
    max_time   = '6.h'
}

aws {
    client {
        endpoint = "https://s3.ugent.be"
        protocol = "https"
        s3PathStyleAccess = true
        connectionTimeout = 60000
    }
}
